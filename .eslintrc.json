{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
			"linebreak-style": 0,
			"indent": [2, "tab"],
			"no-tabs": 0,
			"no-underscore-dangle": "off",
			"no-useless-return": "off",
			"no-param-reassign": 0
    }
}
